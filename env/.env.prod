# FANZ Production Environment
NODE_ENV=production
PORT=5000

# Database
DATABASE_URL=postgresql://prod-cluster:5432/fanzdash_prod
POSTGRES_DB=fanzdash_prod
POSTGRES_USER=fanzdash_prod
POSTGRES_PASSWORD=${POSTGRES_PROD_PASSWORD}

# AI Services
OPENAI_API_KEY=${OPENAI_API_KEY_PROD}
OPENAI_MODEL=gpt-4o
ANTHROPIC_API_KEY=${ANTHROPIC_API_KEY_PROD}

# Media & Storage
AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_PROD}
AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY_PROD}
S3_BUCKET=fanzdash-prod-media
CLOUDFLARE_R2_ENDPOINT=${R2_ENDPOINT_PROD}
CLOUDFLARE_R2_ACCESS_KEY=${R2_ACCESS_KEY_PROD}
CLOUDFLARE_R2_SECRET_KEY=${R2_SECRET_KEY_PROD}

# Authentication & Security
JWT_SECRET=${JWT_SECRET_PROD}
SESSION_SECRET=${SESSION_SECRET_PROD}
ENCRYPTION_KEY=${ENCRYPTION_KEY_PROD}

# Payment Gateways (Production)
CCBILL_CLIENT_ID=${CCBILL_CLIENT_ID_PROD}
CCBILL_CLIENT_SECRET=${CCBILL_CLIENT_SECRET_PROD}
SEGPAY_API_KEY=${SEGPAY_API_KEY_PROD}
EPOCH_API_KEY=${EPOCH_API_KEY_PROD}
VENDO_API_KEY=${VENDO_API_KEY_PROD}
VEROTEL_API_KEY=${VEROTEL_API_KEY_PROD}

# Observability
OTEL_SERVICE_NAME=fanzdash-prod
OTEL_EXPORTER_OTLP_ENDPOINT=${OTEL_ENDPOINT_PROD}
LOG_LEVEL=warn

# Feature Flags
ENABLE_DEBUG_UI=false
ENABLE_MOCK_PAYMENTS=false
ENABLE_AI_PLAYGROUND=false
ENABLE_BETA_FEATURES=false
ENABLE_PERFORMANCE_MONITORING=true