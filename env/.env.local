# FANZ Development Environment
NODE_ENV=development
PORT=5000

# Database
DATABASE_URL=postgresql://localhost:5432/fanzdash_dev
POSTGRES_DB=fanzdash_dev
POSTGRES_USER=fanzdash
POSTGRES_PASSWORD=secure_dev_password

# AI Services
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
ANTHROPIC_API_KEY=your_anthropic_key_here

# Media & Storage
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
S3_BUCKET=fanzdash-dev-media
CLOUDFLARE_R2_ENDPOINT=your_r2_endpoint
CLOUDFLARE_R2_ACCESS_KEY=your_r2_key
CLOUDFLARE_R2_SECRET_KEY=your_r2_secret

# Authentication & Security
JWT_SECRET=dev_jwt_secret_change_in_prod
SESSION_SECRET=dev_session_secret_change_in_prod
ENCRYPTION_KEY=dev_encryption_key_32_chars_long

# Payment Gateways (Development)
CCBILL_CLIENT_ID=your_ccbill_client_id
CCBILL_CLIENT_SECRET=your_ccbill_client_secret
SEGPAY_API_KEY=your_segpay_api_key
EPOCH_API_KEY=your_epoch_api_key

# Observability
OTEL_SERVICE_NAME=fanzdash
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
LOG_LEVEL=debug

# Development Features
ENABLE_DEBUG_UI=true
ENABLE_MOCK_PAYMENTS=true
ENABLE_AI_PLAYGROUND=true