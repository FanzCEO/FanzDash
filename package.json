  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:backend": "NODE_ENV=development tsx server/index.ts",
    "dev:frontend": "vite",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:warp": "vite build && echo 'Warp build completed'",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "sync": "tsx scripts/platform-sync.ts sync",
    "sync:health": "tsx scripts/platform-sync.ts health",
    "sync:backup": "tsx scripts/platform-sync.ts backup",
    "deploy:replit": "npm run build && npm run sync",
    "deploy:warp": "npm run build:warp && wrangler deploy --config warp.toml --compatibility-date 2024-01-01",
    "dev:warp": "wrangler dev --config warp.toml",
    "preview:warp": "wrangler preview --config warp.toml",
    "health-check": "curl -f http://localhost:${PORT:-3000}/healthz || exit 1",
    "system-check": "curl -s http://localhost:${PORT:-3000}/system | jq .",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "setup:ssh": "node scripts/setup-ssh.js",
    "verify:ssh": "node scripts/verify-ssh.js",
    "setup:dev": "npm run setup:ssh && npm install --legacy-peer-deps && npm run db:push"
  },
