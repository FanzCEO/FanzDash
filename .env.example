ENABLE_VR_FEATURES=true
ENABLE_AI_COMPANIONS=true
ENABLE_GEO_COLLABORATION=true
ENABLE_CRYPTO_PAYMENTS=true
ENABLE_LOVENSE_INTEGRATION=false