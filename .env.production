PREDICTIVE_ANALYTICS_ENABLED=true

# ==================== DEVELOPMENT TOGGLES (SET TO FALSE IN PRODUCTION) ====================
DEBUG_MODE=false
VERBOSE_LOGGING=false
ENABLE_DEV_TOOLS=false